{
    "name": "zlab-style-guide",
    "version": "1.3.2",
    "description": "Zeppelin Lab GmbH Style Guide",
    "private": true,
    "main": "dist",
    "repository": {
        "type": "git",
        "url": "https://github.com/Klickrent/zlab-style-guide.git"
    },
    "scripts": {
        "init": "npm install",
        "build": "npm run generate-icon-font && npm run webpack && npm run copy-font-folder && npm run copy-style-folder",
        "webpack": "webpack",
        "watch": "nodemon -e scss -x \"npm run build\"",
        "generate-icon-font": "rm src/fonts/zamicon-v*.* && node svg2font.js src/images/zamicons -of src/fonts -os src/scss -n zamicon -p zamicon- -b zamicon",
        "copy-font-folder": "rm -rf dist/fonts/; cp -R src/fonts/ dist/",
        "copy-style-folder": "rm -f docs/css/main.css; cp dist/css/main.css docs/css"
    },
    "devDependencies": {
        "argparse": "^1",
        "autoprefixer": "^9.1.3",
        "bootstrap": "^4.1.3",
        "bootstrap-material-design": "4.1.1",
        "bootstrap-sass": ">=3.4.1",
        "cross-env": "^5.2.0",
        "css-loader": "2.1.1",
        "cssesc": "^1",
        "extract-text-webpack-plugin": "next",
        "jquery": ">=3.4.0",
        "mkdirp": "^0.5.1",
        "mini-css-extract-plugin": "0.6.0",
        "node-sass": "^4.11.0",
        "nodemon": "^1.18.11",
        "popper.js": "^1.14.5",
        "sanitize-filename": "^1.6",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "svg2ttf": "^4",
        "svgicons2svgfont": "^9",
        "ttf2eot": "^2",
        "ttf2woff": "^2",
        "ttf2woff2": "^3",
        "url-loader": "1.1.2",
        "webpack": "4.30.0",
        "webpack-cli": "^3.3.0"
    }
}
